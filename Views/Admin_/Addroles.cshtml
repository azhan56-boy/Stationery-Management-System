@model Stationery_Management_System.Models.UserRoles

@{
    ViewData["Title"] = "Addroles";
    Layout = "~/Views/Shared/Admin-layout.cshtml";
}

<div class="container">
<h1>Add Placement</h1>

<h4>UserRoles</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Addroles">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="UserRoleName" class="control-label"></label>
                <input asp-for="UserRoleName" class="form-control" />
                <span asp-validation-for="UserRoleName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
</div>

